---
apiVersion: v1
baseDomain: ${domain}
compute: 
- hyperthreading: Enabled 
  name: worker
  replicas: ${workers_number} 
controlPlane: 
  hyperthreading: Enabled 
  name: master
  replicas: ${masters_number} 
metadata:
  name: ${clusterid} 
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14
    hostPrefix: 23 
  networkType: OVNKubernetes 
  serviceNetwork: 
  - 172.30.0.0/16
platform:
  none: {}
pullSecret: '{"auths": ...}' 
sshKey: '${public_key}' 